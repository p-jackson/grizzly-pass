{
  "name": "grizzly-pass",
  "version": "1.0.0-alpha.5",
  "private": true,
  "homepage": "https://grizzly-pass.surge.sh",
  "dependencies": {
    "debug": "^3.0.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "react": "^15.6.1",
    "react-addons-shallow-compare": "^15.6.0",
    "react-aria-menubutton": "^5.0.2",
    "react-dates": "^12.6.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/enzyme": "^2.8.8",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/jest": "^20.0.8",
    "@types/lodash": "^4.14.74",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.0.5",
    "@types/react-aria-menubutton": "^5.0.0",
    "@types/react-dates": "^12.1.4",
    "@types/react-dom": "^15.5.4",
    "@types/react-redux": "^5.0.9",
    "@types/webpack": "^3.0.11",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.1.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.5.3",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-jest": "^21.0.1",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-language-service": "^0.9.6",
    "typescript": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  },
  "scripts": {
    "start": "webpack-dev-server --progress -d",
    "build":
      "webpack --progress -p --env.production && ncp CNAME.staging.txt build/CNAME",
    "build-production":
      "webpack --progress -p --env.production && ncp CNAME.prod.txt build/CNAME",
    "test": "jest --watch",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts{,x}"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setup-tests.ts",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json"],
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!src/**/*.stories.{ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/__mocks__/style-mock.ts"
    }
  }
}
